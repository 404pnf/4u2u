<?php

function disney_menu(){
	
	$items =array();
	$items['disney']=array(
	    'title' => '迪士尼英语家庭版',
	    'page callback' => 'disney_list_page',
		'file' => 'disney.pages.inc',
	    'access callback' => true,
	    'type' => MENU_CALLBACK,
	);
   $items['disney/series'] = array(
   	'title' => '',
   	'page callback' => 'disney_series',
	'file' => 'disney.pages.inc',
    'page arguments' =>array(2),
   	'access callback'=>true,
    'type' => MENU_CALLBACK,
   );
	return $items;
	
}

